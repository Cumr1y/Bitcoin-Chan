modules = ["nodejs-20"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["pkg-config", "cairo", "pango", "libjpeg", "giflib", "librsvg", "pixman", "libuuid"]

[workflows]
runButton = "Discord Bot"

[[workflows.workflow]]
name = "Discord Bot"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm start"
localPort = 3000
externalPort = 80
